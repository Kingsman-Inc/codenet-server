{
  "server_info": {
    "name": "CodeNet Server",
    "version": "3.0.0",
    "build_date": "2025-11-21",
    "developer": "CodeNet Inc",
    "license": "MIT"
  },
  "performance": {
    "cache_enabled": true,
    "cache_timeout": 60,
    "max_connections": 1000,
    "request_timeout": 30
  },
  "security": {
    "session_duration_hours": 24,
    "max_failed_attempts": 5,
    "rate_limit_per_minute": 100
  },
  "logging": {
    "level": "INFO",
    "max_log_size_mb": 50,
    "backup_count": 5,
    "log_file_path": "logs/CodeNet_server.log"
  }
}
